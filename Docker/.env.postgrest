# PostgREST connection to Postgres
# Ensure the password matches .env.postgres
PGRST_DB_URI=postgres://admin:__REPLACE_WITH_STRONG_PASSWORD__@postgres:5432/n8n

# Anonymous role and exposed schemas
PGRST_DB_ANON_ROLE=web_anon
PGRST_DB_SCHEMAS=public

# Server port (published at host:3000)
PGRST_SERVER_PORT=3000
